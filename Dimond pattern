#include <iostream>

#include<cmath>

#include<algorithm>

using namespace std;

int main() {

for(int i = 1; i <= 15; i++){

for(int j = 1; j < =15; j++){

int a = 8 - abs(8 - j);

int b= 8 - abs(8 - i);

if (a + b <= 8){

cout<<char(a+ 64)<<" ";

}

else{

cout<<" ";

}

}

cout<<endl;



}

return 0;

}

